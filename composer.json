{
    "name": "pixelopen/kirby-button",
    "version": "1.0.0",
    "type": "kirby-plugin",
    "description": "A plugin for Kirby 3 CMS to add backend button block",
    "homepage": "https://github.com/Pixel-Developpement/kirby-dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Pixel Open",
            "homepage": "https://pixel-open.org/"
        }
    ],
    "keywords": [
        "kirby",
        "kirby-cms",
        "kirby-plugin"
    ],
    "require": {
        "getkirby/composer-installer": "^1.2",
        "getkirby/cms": "^4.0.0-beta.2"
    },
    "extra": {
        "installer-name": "kirby-button"
    },
    "config": {
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    },
    "require-dev": {
        "symplify/easy-coding-standard": "^12.0"
    }
}
